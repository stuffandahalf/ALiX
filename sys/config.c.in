#include <stddef.h>
#include <alix/dev.h>
#include <alix/log.h>

`for BUS in $KERNEL_ROOT_BUS; do
	echo "extern struct dev $BUS;"
done`
struct dev *root_devs[] = {
`for BUS in $KERNEL_ROOT_BUS; do
	if [ $COMMA ]; then
		printf ",\n"
	fi
	printf "\t&%s" "$BUS"
	COMMA=1
done
printf "\n"`
};
long int root_devc = `echo "$KERNEL_ROOT_BUS" | wc -w | tr -d [:space:]`;

void
anounce(void)
{
	klogs(\"ALiX ${SYSTEM_VERSION}\\\\n\");
	klogs(\"Copyright (c) 2022,2023 Gregory Norton\\\\n\");
}
