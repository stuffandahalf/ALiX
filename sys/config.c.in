#include <stddef.h>
#include <alix/bus.h>

`for BUS in $KERNEL_ROOT_BUS; do
	echo "extern struct bus ${BUS}_bus;"
done`
struct bus *bus_root[] = {
`for BUS in $KERNEL_ROOT_BUS; do
	if [ $COMMA ]; then
		printf ",\n"
	fi
	printf "\t&%s_bus" "$BUS"
	COMMA=1
done
printf "\n"`
};
size_t bus_root_count = `echo "$KERNEL_ROOT_BUS" | wc -w`;
